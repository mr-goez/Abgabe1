{
    "name": "beispiel",
    "version": "1.0.0",
    "description": "Beispiel",
    "main": "dist/index.js",
    "scripts": {
        "prestart": "ts-node scripts/init",
        "start": "nodemon -V",
        "pretest": "npm run mongo import && if exist server.log del server.log && ts-node scripts/init",
        "test": "npx --no-install mocha dist\\buch\\rest\\index.spec.js",
        "clean": "ts-node scripts/clean",
        "mailserver": "java -jar C:/Zimmermann/fakesmtp/fakeSMTP-2.0.jar -s -p 25000 -m",
        "fixme": "ts-node scripts/fixme",
        "mongo": "ts-node scripts/mongo",
        "prettier": "npx prettier --write src/**/*.ts",
        "pretsc": "npm run prettier && npm run tslint",
        "tsc": "npx --no-install tsc -w",
        "tslint": "ts-node scripts/tslint",
        "prestartsingle": "ts-node scripts/init",
        "startsingle": "node dist/index.js",
        "starth2": "node --expose-http2 dist/index.js",
        "testreport": "allure serve .\\allure-results",
        "proxy": "ts-node scripts/proxy"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test",
            "pre-push": "npm test"
        }
    },
    "private": true,
    "homepage": "https://www.hs-karlsruhe.de",
    "pre-commit": [
        "node scripts/tslint"
    ],
